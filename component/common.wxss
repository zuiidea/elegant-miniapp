.high-light {
  color: #ea5558;
}

.navigator-hover.transparent {
  background-color: transparent;
}

::-webkit-scrollbar {
  color: transparent;
  width: 0;
  height: 0;
}

/* .text-overflow */

.text-overflow, .text-overflow-2, .text-overflow-3, .text-overflow-5,
.text-overflow-5 {
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-box-orient: vertical;
}

.text-overflow {
  -webkit-line-clamp: 1;
}

.text-overflow-2 {
  -webkit-line-clamp: 2;
}

.text-overflow-3 {
  -webkit-line-clamp: 3;
}

.text-overflow-4 {
  -webkit-line-clamp: 4;
}

.text-overflow-5 {
  -webkit-line-clamp: 5;
}

/* hairline */

.hairline-right, .hairline-bottom, .hairline-left, .hairline-top {
  position: relative;
}

.hairline-right::after, .hairline-bottom::before, .hairline-left::before,
.hairline-top::after {
  content: "";
  position: absolute;
  background-color: #ececec;
  display: block;
  z-index: 5;
}

.hairline-right::after {
  right: 0;
  left: auto;
  top: 0;
  bottom: 0;
  width: 1px;
  transform: scaleX(0.3333);
  transform-origin: 100% 0;
}

.hairline-bottom::before {
  right: 0;
  left: 0;
  top: auto;
  bottom: 0;
  height: 1px;
  transform: scaleY(0.3333);
  transform-origin: 0 100%;
}

.hairline-left::before {
  right: auto;
  left: 0;
  top: 0;
  bottom: 0;
  width: 1px;
  transform: scaleX(0.3333);
  transform-origin: 0 0;
}

.hairline-top::after {
  right: 0;
  left: 0;
  top: 0;
  bottom: auto;
  height: 1px;
  transform: scaleY(0.3333);
  transform-origin: 0 0;
}

.hairline-border {
  position: relative;
}

.hairline-border::after {
  content: " ";
  width: 200%;
  height: 200%;
  position: absolute;
  top: 0;
  left: 0;
  border: 1px solid #ececec;
  transform: scale(0.5);
  transform-origin: 0 0;
  box-sizing: border-box;
}

